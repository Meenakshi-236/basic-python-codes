#grade
marks=int(input("enter the marks"))
if(marks>=90):
    print("A grade")
    print("excellent")
elif(marks>=70 and marks<=89):
    print("B grade")
    print("Good")
elif(marks>=50 and marks<=69):
    print("C grade ")
elif(marks>=40 and marks<=49):
    print("D grade ")
elif(marks<=40):
    print(" Fail ")
else:
    print("error")
    
